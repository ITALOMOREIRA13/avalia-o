/*8. Faça um programa que exiba os números de 1 (inclusive) a 10 (inclusive) em ordem decrescente*/

programa
{
	
	funcao inicio()
	{
 
 		inteiro x=1, total, t2, t3, t4, t5, t6, t7, t8, t9
		

		 escreva ("número de 1 a 10 em ordem decrescente, ","\n")

 
		total= x+1
		t2 = x +2
		t3 = x +3 
		t4 = x +4
		t5 = x +5
		t6 = x +6
		t7 = x +7
		t8 = x +8
		t9 = x +9

 
 			 escreva (t9,"\n", t8, "\n", t7,"\n", t6,"\n", t5,"\n", t4,"\n", t3,"\n", t2,"\n", total,"\n",total, "\n", x)
	}


}
